{
    "name": "wudg/pdf-images",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "Convert pdf to images, combine images into pdf components, rely on imagick extension",
    "autoload": {
        "psr-4": {
            "Wudg\\PdfImages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-bcmath": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "hyperf/contract": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "psr/container": "^1.0 || ^2.0",
        "ramsey/uuid": "^4.9"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0",
        "swoole/ide-helper": "dev-master"
    },
    "suggest": {
        "swow/swow": "Required to create swow components."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Wudg\\PdfImages\\ConfigProvider"
        }
    }
}
